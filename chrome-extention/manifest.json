{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "1.0",
    "description": "A simple Pomodoro timer by Srikrushna Pal",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "128": "icon.png"
      }
    },
    "permissions": [ "storage", "alarms", "notifications", "activeTab" ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }