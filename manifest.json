{
    "manifest_version": 3,
    "name": "Pomodoro Timer",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "128": "icon.png"
      }
    },
    "permissions": ["activeTab","storage", "alarms", "notifications"],
    "icons": {
      "128": "icon.png"
    }
  }